{
    "name": "tms/consent",
    "type": "neos-plugin",
    "description": "Simple, lightweight and customizable consent manager for Neos CMS based on Klaro!",
    "keywords": ["neoscms", "gdpr", "eprivacy", "consent-management", "cookie", "third-party", "klaro"],
    "license": "MIT",
    "require": {
        "neos/neos": "^4.3 || ^5.0 || ^7.0 || ^8.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "Tms\\Consent\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "loading-order": {
                "after": [
                    "flowpack/neos-matomo"
                ]
            }
        }
    }
}
